    xmlns:tns="http://rosleshoz.gov.ru/fgis-lk/pozvlForestReforestation/types/3.1.1"
    targetNamespace="http://rosleshoz.gov.ru/fgis-lk/pozvlForestReforestation/types/3.1.1"
    elementFormDefault="qualified"
    xmlns:st="http://rosleshoz.gov.ru/fgis-lk/common/simple/types/2.2.1"
